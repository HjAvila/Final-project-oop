<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.finaoop.controllers.ReportsController">
    <VBox layoutX="20" layoutY="20" spacing="12" prefWidth="760" style="-fx-padding:20;">
        <Label text="Reports" styleClass="page-title"/>
        <TableView fx:id="table" prefHeight="420">
            <columns>
                <TableColumn fx:id="colId" text="ID" prefWidth="140"/>
                <TableColumn fx:id="colLocation" text="Location" prefWidth="260"/>
                <TableColumn fx:id="colType" text="Type" prefWidth="140"/>
                <TableColumn fx:id="colStatus" text="Status" prefWidth="120"/>
                <TableColumn fx:id="colDate" text="Date" prefWidth="140"/>
            </columns>
        </TableView>
    </VBox>
</AnchorPane>
